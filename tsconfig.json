{
    "compilerOptions": {
      "module": "commonjs",
      "declaration": true,
      "removeComments": true,
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "target": "es2017",
      "sourceMap": true,
      "outDir": "./commonjs/dist",
      "baseUrl": "./",
      "incremental": true
    },
    "paths": {
      "src/*": ["src/*"]
    },
    "exclude": ["node_modules", "dist"]
}
  